// COLOR CODING:
// Weapons = Light Green
// Health = Light Red
// Armor = Yellow
// Powerups = Light Blue
// Holdables = Light Violet
// Runes = Blue/Red/White

textures/itembases/weapon-gauntlet
{
	qer_editorimage icons/iconw_gauntlet.tga
	qer_trans 0.5
	q3map_lightimage textures/itembases/editor/itembase_green.tga
	surfaceparm nomarks
	surfaceparm nonsolid
	surfaceparm trans
	nomipmaps
	polygonoffset
	{
		clampmap icons/iconw_gauntlet.tga
		blendfunc blend
		rgbGen wave sin 0.5 0.5 0 1 
		tcMod rotate -90
		tcMod stretch sawtooth 0.15 0 0 1 
	}
	{
		clampmap textures/itembases/editor/itembase_green.tga
		blendfunc blend
		tcMod rotate 90
	}
}

