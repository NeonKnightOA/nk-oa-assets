textures/gothic_door/xian_dm3arch
{
	{
		map textures/gothic_door/xian_dm3arch.tga
		rgbGen identity
	}
	{
		map $lightmap 
		blendfunc filter
		rgbGen identity
		tcGen lightmap 
	}
	{
		map gfx/fx/detail/d_rock.tga
		blendfunc gl_dst_color gl_src_color
		tcMod scale 0.1 0.1
		detail
	}
}

