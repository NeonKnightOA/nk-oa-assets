models/players/neko/hair_bl
{
	cull disable
	{
		map models/players/neko/hair_bl.tga
		rgbGen lightingDiffuse
		alphaFunc GE128
	}
	{
		map models/players/neko/hair_spec.tga
		blendfunc gl_src_alpha gl_one
		rgbGen lightingDiffuse
		depthFunc equal
		alphaGen lightingSpecular
		detail
	}
}

